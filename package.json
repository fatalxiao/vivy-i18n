{
    "name": "vivy-i18n",
    "author": "liangxiaojun",
    "version": "3.2.0",
    "description": "A Vivy Plugin to handle i18ns in Vivy model to implement i18n.",
    "main": "lib/index.js",
    "module": "es/index.js",
    "types": "es/index.d.ts",
    "files": [
        "lib",
        "es",
        "src",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "clean": "rimraf lib es",
        "test": "cross-env NODE_ENV=test jest test/**.test.js",
        "build:commonjs": "tsc --module commonjs --declaration false --outDir lib",
        "build:es": "tsc --module esnext --declaration true --outDir es",
        "build": "npm run clean && npm run build:commonjs && npm run build:es",
        "do-publish": "npm run build && npm publish"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fatalxiao/vivy-i18n"
    },
    "keywords": [
        "react",
        "redux",
        "i18n",
        "vivy"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/fatalxiao/vivy-i18n/issues"
    },
    "peerDependencies": {
        "@types/react": "^16.8 || ^17.0 || ^18.0",
        "@types/react-dom": "^16.8 || ^17.0 || ^18.0",
        "react": "^16.8 || ^17.0 || ^18.0",
        "react-dom": "^16.8 || ^17.0 || ^18.0",
        "vivy": "^3"
    },
    "dependencies": {
        "prop-types": ">=15.x",
        "react": ">=16.8.x",
        "react-dom": ">=16.8.x",
        "react-vivy": ">=2.2.x"
    },
    "devDependencies": {
        "@types/react": ">=16.8",
        "@types/react-dom": ">=16.8",
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "@typescript-eslint/parser": "^5.55.0",
        "eslint": "^8.36.0",
        "eslint-plugin-jest": "^27.2.1",
        "jest": "^29.5.0",
        "rimraf": "^4.4.0",
        "typescript": "^5.0.2",
        "vivy": "^3.2.1"
    }
}
